<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
    <head>
    <title></title>
    <meta http-equiv="left_menu_content-Type" left_menu_content="text/html; charset=UTF-8">

    <script type="text/javascript">
        
        $('a.left_menu_content').each(function () {
            $('<div class="fader" />').css('opacity', 0).prependTo(this);
        }).hover(function () {
            $('span', this).animate({
                marginLeft : 5
            }, 250);

            $('.fader', this).animate({
                opacity : 0.15
            });

        }, function () {
            $('span', this).animate({
                marginLeft : 10
            }, 250);

            $('.fader', this).animate({
                opacity : 0
            });
        }).find('span').css('marginLeft', 10);

    </script>
</head>
<body>
<div class="left_menu_content" >

    <ul class="content_shadow">
        <li id="left_menu_link_1"><a id="generalLink" class="left_menu_links" href="#">
                <img class="left_menu_icons" src="static/images/icons/left_profile_menu_icons/profile.png"/>
               <span class="left_hidden_div_1"> </span>
                <span>General</span></a></li>
        <li><a id="photoLink"  class="left_menu_links" href="#">
                <img class="left_menu_icons" src="static/images/icons/left_profile_menu_icons/pictures.png"/>
               <span class="left_hidden_div"> </span>
                <span>Picture</span></a></li>
        <li><a  id="passwordLink" class="left_menu_links" href="#">
                <img class="left_menu_icons"  src="static/images/icons/left_profile_menu_icons/music_box.png"/>
               <span class="left_hidden_div"> </span>
                <span>Password</span></a></li>
        <li><a id="emailLink"  class="left_menu_links" href="#">
                <img class="left_menu_icons" src="static/images/icons/left_profile_menu_icons/television.png"/>
               <span class="left_hidden_div"> </span>
                <span>Email</span></a></li>

    </ul>


</div>

</body>
</html>
