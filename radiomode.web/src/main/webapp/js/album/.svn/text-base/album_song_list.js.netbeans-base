$(document).ready(function() {
 
    
    
    // Paging Parameters
   
    var divId = "#album_song_paging"; 
    var pageNumber = $(".songPageNumberOfAlbum").text();
    
    makeDivPaginate(divId, pageNumber.valueOf(),  1, 5); 
   
   

});